<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:l2"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="l2.MainWindow"
        Title="ConsoleApp1">
  <DockPanel>
    <local:ViewEditor Name="viewEditor" DockPanel.Dock="Top"/>
      <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
    <Button Name="loadButton" Width="50"  >Load</Button>
        
      </StackPanel>
      
    <local:DynamicTable Name="table" DockPanel.Dock="Bottom" IsReadOnly="True"/>
  </DockPanel>
</Window>
